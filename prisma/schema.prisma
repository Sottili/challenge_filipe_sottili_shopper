generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = "postgresql://admin:admin@localhost:5432/challenge_shopper"
}

model Measurement {
  measure_uuid    String   @id @default(uuid())
  measure_value   Int
  measure_datetime DateTime
  measure_type    String
  image_url       String
  confirmed       Int
  customer Customer[]
}

model Customer {
  customer_code String @id @default(uuid())
  measurement Measurement[]
}
