generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = "postgresql://admin:admin@localhost:5432/challenge_shopper"
}

model Measurement {
  id              Int      @id @default(autoincrement())
  measure_uuid    String   @unique
  measure_value   Int
  measure_datetime DateTime
  measure_type    String
  image_url       String
  confirmed       Int
  customer_code   String
}